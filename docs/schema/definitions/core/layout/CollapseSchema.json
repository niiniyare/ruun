{
  "type": "object",
  "properties": {
    "$$id": {
      "type": "string",
      "description": "The unique id of the component, mainly used to locate json nodes in the page designer"
    },
    "className": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Container css class name"
    },
    "$ref": {
      "type": "string",
      "description": "Used together with definitions, an infinite loop renderer can be implemented."
    },
    "disabled": {
      "type": "boolean",
      "description": "Whether to disable"
    },
    "disabledOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to disable expressions"
    },
    "hidden": {
      "type": "boolean",
      "description": "Whether to hide"
    },
    "hiddenOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to hide expressions"
    },
    "visible": {
      "type": "boolean",
      "description": "Whether to display"
    },
    "visibleOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions"
    },
    "id": {
      "type": "string",
      "description": "The unique id of the component, mainly used for log collection"
    },
    "onEvent": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "weight": {
            "type": "number"
          },
          "actions": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/ListenerAction"
            }
          },
          "debounce": {
            "$ref": "#/definitions/debounceConfig"
          },
          "track": {
            "$ref": "#/definitions/trackConfig"
          }
        },
        "required": [
          "actions"
        ],
        "additionalProperties": false
      },
      "description": "Event action configuration"
    },
    "static": {
      "type": "boolean",
      "description": "Whether to display statically"
    },
    "staticOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions statically"
    },
    "staticPlaceholder": {
      "type": "string",
      "description": "Static display of empty value placeholder"
    },
    "staticClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display form item class name"
    },
    "staticLabelClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Label class name"
    },
    "staticInputClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Value class name"
    },
    "staticSchema": {},
    "style": {
      "type": "object",
      "description": "Component style"
    },
    "editorSetting": {
      "type": "object",
      "properties": {
        "behavior": {
          "type": "string",
          "description": "Component behavior and usage, such as create, update, remove"
        },
        "displayName": {
          "type": "string",
          "description": "Component name, usually the business name for easy location"
        },
        "mock": {
          "description": "Editor fake data for easy display"
        }
      },
      "description": "Editor configuration, can be ignored at runtime"
    },
    "useMobileUI": {
      "type": "boolean",
      "description": "Can be used to turn off mobile style at the component level"
    },
    "testIdBuilder": {
      "$ref": "#/definitions/TestIdBuilder"
    },
    "type": {
      "type": "string",
      "const": "collapse",
      "description": "Specify as folder type"
    },
    "testid": {
      "type": "string"
    },
    "key": {
      "type": "string",
      "description": "logo"
    },
    "headerPosition": {
      "type": "string",
      "enum": [
        "top",
        "bottom"
      ],
      "description": "title placement"
    },
    "header": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "$ref": "#/definitions/SchemaCollection"
        }
      ],
      "description": "title"
    },
    "body": {
      "$ref": "#/definitions/SchemaCollection",
      "description": "content area"
    },
    "bodyClassName": {
      "type": "string",
      "description": "Configure Body container className"
    },
    "collapsable": {
      "type": "boolean",
      "description": "Is it foldable?"
    },
    "collapsed": {
      "type": "boolean",
      "description": "Whether to collapse by default"
    },
    "showArrow": {
      "type": "boolean",
      "description": "Whether the icon is displayed"
    },
    "expandIcon": {
      "$ref": "#/definitions/SchemaObject",
      "description": "Custom toggle icon"
    },
    "headingClassName": {
      "type": "string",
      "description": "Title CSS class name"
    },
    "collapseHeader": {
      "$ref": "#/definitions/SchemaTpl",
      "description": "Collapse title"
    },
    "size": {
      "type": "string",
      "enum": [
        "xs",
        "sm",
        "md",
        "lg",
        "base"
      ],
      "description": "Control size"
    },
    "mountOnEnter": {
      "type": "boolean",
      "description": "Content is loaded when clicked"
    },
    "unmountOnExit": {
      "type": "boolean",
      "description": "When the card is hidden, the content is destroyed."
    },
    "divideLine": {
      "type": "boolean",
      "description": "Title content dividing line"
    }
  },
  "required": [
    "body",
    "type"
  ],
  "additionalProperties": false,
  "description": "Collapse collapse renderer, format description. Documentation: https://aisuda.bce.baidu.com/amis/zh-CN/components/collapse",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./CollapseSchema.json"
}