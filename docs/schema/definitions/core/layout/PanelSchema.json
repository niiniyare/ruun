{
  "type": "object",
  "properties": {
    "$$id": {
      "type": "string",
      "description": "The unique id of the component, mainly used to locate json nodes in the page designer"
    },
    "className": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Container css class name"
    },
    "$ref": {
      "type": "string",
      "description": "Used together with definitions, an infinite loop renderer can be implemented."
    },
    "disabled": {
      "type": "boolean",
      "description": "Whether to disable"
    },
    "disabledOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to disable expressions"
    },
    "hidden": {
      "type": "boolean",
      "description": "Whether to hide"
    },
    "hiddenOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to hide expressions"
    },
    "visible": {
      "type": "boolean",
      "description": "Whether to display"
    },
    "visibleOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions"
    },
    "id": {
      "type": "string",
      "description": "The unique id of the component, mainly used for log collection"
    },
    "onEvent": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "weight": {
            "type": "number"
          },
          "actions": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/ListenerAction"
            }
          },
          "debounce": {
            "$ref": "#/definitions/debounceConfig"
          },
          "track": {
            "$ref": "#/definitions/trackConfig"
          }
        },
        "required": [
          "actions"
        ],
        "additionalProperties": false
      },
      "description": "Event action configuration"
    },
    "static": {
      "type": "boolean",
      "description": "Whether to display statically"
    },
    "staticOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions statically"
    },
    "staticPlaceholder": {
      "type": "string",
      "description": "Static display of empty value placeholder"
    },
    "staticClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display form item class name"
    },
    "staticLabelClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Label class name"
    },
    "staticInputClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Value class name"
    },
    "staticSchema": {},
    "style": {
      "type": "object",
      "description": "Component style"
    },
    "editorSetting": {
      "type": "object",
      "properties": {
        "behavior": {
          "type": "string",
          "description": "Component behavior and usage, such as create, update, remove"
        },
        "displayName": {
          "type": "string",
          "description": "Component name, usually the business name for easy location"
        },
        "mock": {
          "description": "Editor fake data for easy display"
        }
      },
      "description": "Editor configuration, can be ignored at runtime"
    },
    "useMobileUI": {
      "type": "boolean",
      "description": "Can be used to turn off mobile style at the component level"
    },
    "testIdBuilder": {
      "$ref": "#/definitions/TestIdBuilder"
    },
    "type": {
      "type": "string",
      "const": "panel",
      "description": "Specified as Panel renderer."
    },
    "testid": {
      "type": "string"
    },
    "actions": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/ActionSchema"
      },
      "description": "Button collection"
    },
    "actionsClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Button collection outer class name"
    },
    "body": {
      "$ref": "#/definitions/SchemaCollection",
      "description": "content area"
    },
    "bodyClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Configure Body container className"
    },
    "footer": {
      "$ref": "#/definitions/SchemaCollection",
      "description": "bottom content area"
    },
    "footerClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Configure footer container className"
    },
    "footerWrapClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "The class name of the outer div of footer and actions."
    },
    "header": {
      "$ref": "#/definitions/SchemaCollection",
      "description": "Header content, and title."
    },
    "headerClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Configure header container className"
    },
    "title": {
      "$ref": "#/definitions/SchemaTpl",
      "description": "Panel title"
    },
    "affixFooter": {
      "anyOf": [
        {
          "type": "boolean"
        },
        {
          "type": "string",
          "const": "always"
        }
      ],
      "description": "Fixed bottom, configured when you want to fix the button at the bottom."
    },
    "collapsible": {
      "type": "boolean",
      "description": "\\ Foldable. Letâ€™s implement it simply"
    },
    "subFormMode": {
      "type": "string",
      "enum": [
        "normal",
        "inline",
        "horizontal"
      ],
      "description": "Configure the default display mode of subform items."
    },
    "subFormHorizontal": {
      "$ref": "#/definitions/FormHorizontal",
      "description": "If it is horizontal typesetting, this attribute can refine the left and right width ratio of horizontal typesetting."
    },
    "headerControlClassName": {
      "type": "string",
      "description": "Appearance configuration classname"
    },
    "bodyControlClassName": {
      "type": "string"
    },
    "actionsControlClassName": {
      "type": "string"
    }
  },
  "required": [
    "actionsControlClassName",
    "bodyControlClassName",
    "headerControlClassName",
    "type"
  ],
  "additionalProperties": false,
  "description": "Panel renderer. Documentation: https://aisuda.bce.baidu.com/amis/zh-CN/components/panel",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./PanelSchema.json"
}