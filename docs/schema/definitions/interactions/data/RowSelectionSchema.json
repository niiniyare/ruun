{
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "description": "Selection type single selection/multiple selection"
    },
    "keyField": {
      "type": "string",
      "description": "The key value corresponding to the data source"
    },
    "disableOn": {
      "type": "string",
      "description": "Whether the row disables the expression"
    },
    "selections": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/RowSelectionOptionsSchema"
      },
      "description": "Custom selection menu"
    },
    "selectedRowKeys": {
      "type": "array",
      "items": {
        "type": [
          "string",
          "number"
        ]
      },
      "description": "Selected key value"
    },
    "selectedRowKeysExpr": {
      "type": "string",
      "description": "Selected key value expression"
    },
    "columnWidth": {
      "type": "number",
      "description": "Selected key value expression"
    },
    "rowClick": {
      "type": "boolean",
      "description": "Whether clicking on a row triggers selection or deselection"
    }
  },
  "required": [
    "type"
  ],
  "additionalProperties": false,
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./RowSelectionSchema.json"
}