{
  "type": "object",
  "properties": {
    "$$id": {
      "type": "string",
      "description": "The unique id of the component, mainly used to locate json nodes in the page designer"
    },
    "className": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Outer CSS class name"
    },
    "$ref": {
      "type": "string",
      "description": "Used together with definitions, an infinite loop renderer can be implemented."
    },
    "disabled": {
      "type": "boolean",
      "description": "Whether to disable"
    },
    "disabledOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to disable expressions"
    },
    "hidden": {
      "type": "boolean",
      "description": "Whether to hide"
    },
    "hiddenOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to hide expressions"
    },
    "visible": {
      "type": "boolean",
      "description": "Whether to display"
    },
    "visibleOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions"
    },
    "id": {
      "type": "string",
      "description": "The unique id of the component, mainly used for log collection"
    },
    "onEvent": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "weight": {
            "type": "number"
          },
          "actions": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/ListenerAction"
            }
          },
          "debounce": {
            "$ref": "#/definitions/debounceConfig"
          },
          "track": {
            "$ref": "#/definitions/trackConfig"
          }
        },
        "required": [
          "actions"
        ],
        "additionalProperties": false
      },
      "description": "Event action configuration"
    },
    "static": {
      "type": "boolean",
      "description": "Whether to display statically"
    },
    "staticOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions statically"
    },
    "staticPlaceholder": {
      "type": "string",
      "description": "Static display of empty value placeholder"
    },
    "staticClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display form item class name"
    },
    "staticLabelClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Label class name"
    },
    "staticInputClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Value class name"
    },
    "staticSchema": {},
    "style": {
      "type": "object",
      "description": "Component style"
    },
    "editorSetting": {
      "type": "object",
      "properties": {
        "behavior": {
          "type": "string",
          "description": "Component behavior and usage, such as create, update, remove"
        },
        "displayName": {
          "type": "string",
          "description": "Component name, usually the business name for easy location"
        },
        "mock": {
          "description": "Editor fake data for easy display"
        }
      },
      "description": "Editor configuration, can be ignored at runtime"
    },
    "useMobileUI": {
      "type": "boolean",
      "description": "Can be used to turn off mobile style at the component level"
    },
    "testIdBuilder": {
      "$ref": "#/definitions/TestIdBuilder"
    },
    "type": {
      "type": "string",
      "enum": [
        "images",
        "static-images"
      ],
      "description": "Specify as picture gallery renderer"
    },
    "testid": {
      "type": "string"
    },
    "defaultImage": {
      "$ref": "#/definitions/SchemaUrlPath",
      "description": "Default image address"
    },
    "placeholder": {
      "type": "string",
      "description": "Shown when list is empty"
    },
    "delimiter": {
      "type": "string",
      "description": "Concatenation of configuration values"
    },
    "thumbMode": {
      "type": "string",
      "enum": [
        "w-full",
        "h-full",
        "contain",
        "cover"
      ],
      "description": "Preview mode"
    },
    "thumbRatio": {
      "type": "string",
      "enum": [
        "1:1",
        "4:3",
        "16:9"
      ],
      "description": "Preview ratio"
    },
    "name": {
      "type": "string",
      "description": "Associated field name, you can also directly configure src"
    },
    "value": {},
    "source": {
      "type": "string"
    },
    "options": {
      "type": "array",
      "items": {}
    },
    "src": {
      "type": "string",
      "description": "Image address, reads the image property from data by default. If not, please configure it, such as "
    },
    "originalSrc": {
      "type": "string",
      "description": "Large image address, uses src property if not set. If not, please configure it, such as: "
    },
    "enlargeAble": {
      "type": "boolean",
      "description": "Whether to enable the magnification function."
    },
    "enlargetWithImages": {
      "type": "boolean",
      "description": "Whether to display the picture gallery when zooming in"
    },
    "showDimensions": {
      "type": "boolean",
      "description": "Whether to display dimensions."
    },
    "listClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "List CSS class names"
    },
    "imageGallaryClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Enlarge detailed image CSS class name"
    },
    "showToolbar": {
      "type": "boolean",
      "description": "Whether to display the image toolbar"
    },
    "toolbarActions": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/ImageToolbarAction"
      },
      "description": "Toolbar configuration"
    }
  },
  "required": [
    "type"
  ],
  "additionalProperties": false,
  "description": "Picture gallery display control. Documentation: https://aisuda.bce.baidu.com/amis/zh-CN/components/images",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./ImagesSchema.json"
}