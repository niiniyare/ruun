{
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "testid": {
      "type": "string"
    },
    "$$id": {
      "type": "string",
      "description": "The unique id of the component, mainly used to locate json nodes in the page designer"
    },
    "className": {
      "type": "string"
    },
    "$ref": {
      "type": "string",
      "description": "Used together with definitions, an infinite loop renderer can be implemented."
    },
    "disabled": {
      "type": "boolean"
    },
    "disabledOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to disable expressions"
    },
    "hidden": {
      "type": "boolean",
      "description": "Whether to hide"
    },
    "hiddenOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to hide expressions"
    },
    "visible": {
      "type": "boolean",
      "description": "Whether to display"
    },
    "visibleOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions"
    },
    "id": {
      "type": "string",
      "description": "The unique id of the component, mainly used for log collection"
    },
    "onEvent": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "weight": {
            "type": "number"
          },
          "actions": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/ListenerAction"
            }
          },
          "debounce": {
            "$ref": "#/definitions/debounceConfig"
          },
          "track": {
            "$ref": "#/definitions/trackConfig"
          }
        },
        "required": [
          "actions"
        ],
        "additionalProperties": false
      },
      "description": "Event action configuration"
    },
    "static": {
      "type": "boolean",
      "description": "Whether to display statically"
    },
    "staticOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions statically"
    },
    "staticPlaceholder": {
      "type": "string",
      "description": "Static display of empty value placeholder"
    },
    "staticClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display form item class name"
    },
    "staticLabelClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Label class name"
    },
    "staticInputClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Value class name"
    },
    "staticSchema": {},
    "style": {
      "type": "object",
      "description": "Component style"
    },
    "editorSetting": {
      "type": "object",
      "properties": {
        "behavior": {
          "type": "string",
          "description": "Component behavior and usage, such as create, update, remove"
        },
        "displayName": {
          "type": "string",
          "description": "Component name, usually the business name for easy location"
        },
        "mock": {
          "description": "Editor fake data for easy display"
        }
      },
      "description": "Editor configuration, can be ignored at runtime"
    },
    "useMobileUI": {
      "type": "boolean",
      "description": "Can be used to turn off mobile style at the component level"
    },
    "testIdBuilder": {
      "$ref": "#/definitions/TestIdBuilder"
    },
    "label": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "$ref": "#/definitions/SchemaCollection"
        }
      ],
      "description": "text description"
    },
    "icon": {
      "anyOf": [
        {
          "$ref": "#/definitions/SchemaIcon"
        },
        {
          "type": "array",
          "items": {
            "$ref": "#/definitions/IconItemSchema"
          }
        }
      ],
      "description": "Icon class name, refer to fontawesome 4."
    },
    "to": {
      "$ref": "#/definitions/SchemaUrlPath"
    },
    "target": {
      "type": "string"
    },
    "unfolded": {
      "type": "boolean"
    },
    "active": {
      "type": "boolean"
    },
    "defer": {
      "type": "boolean"
    },
    "deferApi": {
      "$ref": "#/definitions/SchemaApi"
    },
    "children": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/NavItemSchema"
      }
    },
    "key": {
      "type": "string"
    },
    "disabledTip": {
      "type": "string"
    },
    "mode": {
      "type": "string"
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./NavItemSchema.json"
}