{
  "type": "object",
  "properties": {
    "loadingConfig": {
      "type": "object",
      "properties": {
        "root": {
          "type": "string"
        },
        "show": {
          "type": "boolean"
        }
      },
      "additionalProperties": false
    },
    "$$id": {
      "type": "string",
      "description": "The unique id of the component, mainly used to locate json nodes in the page designer"
    },
    "className": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Container css class name"
    },
    "$ref": {
      "type": "string",
      "description": "Used together with definitions, an infinite loop renderer can be implemented."
    },
    "disabled": {
      "type": "boolean",
      "description": "Whether to disable"
    },
    "disabledOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to disable expressions"
    },
    "hidden": {
      "type": "boolean",
      "description": "Whether to hide"
    },
    "hiddenOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to hide expressions"
    },
    "visible": {
      "type": "boolean",
      "description": "Whether to display"
    },
    "visibleOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions"
    },
    "id": {
      "type": "string",
      "description": "The unique id of the component, mainly used for log collection"
    },
    "onEvent": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "weight": {
            "type": "number"
          },
          "actions": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/ListenerAction"
            }
          },
          "debounce": {
            "$ref": "#/definitions/debounceConfig"
          },
          "track": {
            "$ref": "#/definitions/trackConfig"
          }
        },
        "required": [
          "actions"
        ],
        "additionalProperties": false
      },
      "description": "Event action configuration"
    },
    "static": {
      "type": "boolean",
      "description": "Whether to display statically"
    },
    "staticOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions statically"
    },
    "staticPlaceholder": {
      "type": "string",
      "description": "Static display of empty value placeholder"
    },
    "staticClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display form item class name"
    },
    "staticLabelClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Label class name"
    },
    "staticInputClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Value class name"
    },
    "staticSchema": {},
    "style": {
      "type": "object",
      "description": "Component style"
    },
    "editorSetting": {
      "type": "object",
      "properties": {
        "behavior": {
          "type": "string",
          "description": "Component behavior and usage, such as create, update, remove"
        },
        "displayName": {
          "type": "string",
          "description": "Component name, usually the business name for easy location"
        },
        "mock": {
          "description": "Editor fake data for easy display"
        }
      },
      "description": "Editor configuration, can be ignored at runtime"
    },
    "useMobileUI": {
      "type": "boolean",
      "description": "Can be used to turn off mobile style at the component level"
    },
    "testIdBuilder": {
      "$ref": "#/definitions/TestIdBuilder"
    },
    "type": {
      "type": "string",
      "const": "tasks",
      "description": "Specify as task type"
    },
    "testid": {
      "type": "string"
    },
    "btnClassName": {
      "$ref": "#/definitions/SchemaClassName"
    },
    "btnText": {
      "type": "string",
      "description": "Action button text"
    },
    "checkApi": {
      "$ref": "#/definitions/SchemaApi",
      "description": "API used to obtain task status. It will not be sent when there is no ongoing task."
    },
    "interval": {
      "type": "number",
      "description": "When a task is in progress, it will be detected again at regular intervals, and the time interval is configured through this item. The default is 3s.",
      "default": 3000
    },
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "key": {
            "type": "string",
            "description": "Task key value, please uniquely distinguish it"
          },
          "label": {
            "type": "string",
            "description": "Task name"
          },
          "remark": {
            "type": "string",
            "description": "Current task status, supports html"
          },
          "status": {
            "type": "number",
            "enum": [
              0,
              1,
              2,
              3,
              4,
              5
            ],
            "description": "3: There is an error and cannot be retried. 4: Ended normally. Task status: 0: Initial status, inoperable. 5: There is an error and you can try again.1: Ready, operable state. 2: In progress, not over yet."
          }
        },
        "additionalProperties": false
      }
    },
    "name": {
      "$ref": "#/definitions/SchemaName"
    },
    "operationLabel": {
      "type": "string",
      "description": "Operation column description"
    },
    "reSubmitApi": {
      "$ref": "#/definitions/SchemaApi",
      "description": "If the task fails and can be retried, this API will be used when submitting."
    },
    "remarkLabel": {
      "type": "string",
      "description": "Note column description"
    },
    "retryBtnClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Configure container retry button className"
    },
    "retryBtnText": {
      "type": "string",
      "description": "Retry action button text"
    },
    "statusLabel": {
      "type": "string",
      "description": "Status column description"
    },
    "statusLabelMap": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "The status displays the corresponding class name configuration.",
      "default": [
        "label-warning",
        "label-info",
        "label-success",
        "label-danger",
        "label-default",
        "label-danger"
      ]
    },
    "statusTextMap": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "The text display configuration corresponding to the status display."
    },
    "submitApi": {
      "$ref": "#/definitions/SchemaApi",
      "description": "API used to submit tasks"
    },
    "tableClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Configure table className"
    },
    "taskNameLabel": {
      "type": "string",
      "description": "Task name column description"
    },
    "initialStatusCode": {
      "type": "number"
    },
    "readyStatusCode": {
      "type": "number"
    },
    "loadingStatusCode": {
      "type": "number"
    },
    "canRetryStatusCode": {
      "type": "number"
    },
    "finishStatusCode": {
      "type": "number"
    },
    "errorStatusCode": {
      "type": "number"
    }
  },
  "required": [
    "type"
  ],
  "additionalProperties": false,
  "description": "Tasks renderer, format description document: https://aisuda.bce.baidu.com/amis/zh-CN/components/tasks",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./TasksSchema.json"
}