{
  "type": "object",
  "properties": {
    "loadingConfig": {
      "type": "object",
      "properties": {
        "root": {
          "type": "string"
        },
        "show": {
          "type": "boolean"
        }
      },
      "additionalProperties": false
    },
    "$$id": {
      "type": "string",
      "description": "The unique id of the component, mainly used to locate json nodes in the page designer"
    },
    "className": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Configure container className"
    },
    "$ref": {
      "type": "string",
      "description": "Used together with definitions, an infinite loop renderer can be implemented."
    },
    "disabled": {
      "type": "boolean",
      "description": "Whether to disable"
    },
    "disabledOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to disable expressions"
    },
    "hidden": {
      "type": "boolean",
      "description": "Whether to hide"
    },
    "hiddenOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to hide expressions"
    },
    "visible": {
      "type": "boolean",
      "description": "Whether to display"
    },
    "visibleOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions"
    },
    "id": {
      "type": "string",
      "description": "The unique id of the component, mainly used for log collection"
    },
    "onEvent": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "weight": {
            "type": "number"
          },
          "actions": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/ListenerAction"
            }
          },
          "debounce": {
            "$ref": "#/definitions/debounceConfig"
          },
          "track": {
            "$ref": "#/definitions/trackConfig"
          }
        },
        "required": [
          "actions"
        ],
        "additionalProperties": false
      },
      "description": "Event action configuration"
    },
    "static": {
      "type": "boolean",
      "description": "Whether to display statically"
    },
    "staticOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions statically"
    },
    "staticPlaceholder": {
      "type": "string",
      "description": "Static display of empty value placeholder"
    },
    "staticClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display form item class name"
    },
    "staticLabelClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Label class name"
    },
    "staticInputClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Value class name"
    },
    "staticSchema": {},
    "style": {
      "type": "object",
      "description": "Custom style"
    },
    "editorSetting": {
      "type": "object",
      "properties": {
        "behavior": {
          "type": "string",
          "description": "Component behavior and usage, such as create, update, remove"
        },
        "displayName": {
          "type": "string",
          "description": "Component name, usually the business name for easy location"
        },
        "mock": {
          "description": "Editor fake data for easy display"
        }
      },
      "description": "Editor configuration, can be ignored at runtime"
    },
    "useMobileUI": {
      "type": "boolean",
      "description": "Can be used to turn off mobile style at the component level"
    },
    "testIdBuilder": {
      "$ref": "#/definitions/TestIdBuilder"
    },
    "type": {
      "type": "string",
      "const": "page",
      "description": "Specified as the page renderer."
    },
    "testid": {
      "type": "string"
    },
    "title": {
      "type": "string",
      "description": "Page title"
    },
    "subTitle": {
      "type": "string",
      "description": "Page subtitle"
    },
    "remark": {
      "$ref": "#/definitions/SchemaRemark",
      "description": "A small icon will appear next to the page description and title. When placed, the content of this attribute configuration will be displayed."
    },
    "body": {
      "$ref": "#/definitions/SchemaCollection",
      "description": "content area"
    },
    "bodyClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Content area css class name"
    },
    "aside": {
      "$ref": "#/definitions/SchemaCollection",
      "description": "sidebar area"
    },
    "asideResizor": {
      "type": "boolean",
      "description": "Whether the sidebar allows dragging"
    },
    "asideSticky": {
      "type": "boolean",
      "description": "Whether the sidebar content is sticky, that is, does not follow scrolling.",
      "default": true
    },
    "asidePosition": {
      "type": "string",
      "enum": [
        "left",
        "right"
      ],
      "description": "sidebar position",
      "default": "left"
    },
    "asideMinWidth": {
      "type": "number",
      "description": "Sidebar minimum width"
    },
    "asideMaxWidth": {
      "type": "number",
      "description": "Sidebar minimum width"
    },
    "asideClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Sidebar area css class name"
    },
    "css": {
      "type": "object",
      "additionalProperties": {
        "anyOf": [
          {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            }
          },
          {
            "type": "object",
            "additionalProperties": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            }
          }
        ]
      },
      "description": "Custom page level style sheet"
    },
    "mobileCSS": {
      "type": "object",
      "additionalProperties": {
        "anyOf": [
          {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            }
          },
          {
            "type": "object",
            "additionalProperties": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            }
          }
        ]
      },
      "description": "Style sheet under mobile terminal"
    },
    "data": {
      "$ref": "#/definitions/SchemaDefaultData",
      "description": "Page level initial data"
    },
    "headerClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Configure header container className"
    },
    "initApi": {
      "$ref": "#/definitions/SchemaApi",
      "description": "When the page is initialized, you can set up an API to pull it. The data sent will carry the current data data (including address bar parameters), and the obtained data will be merged into data for use within the component."
    },
    "initFetch": {
      "type": "boolean",
      "description": "Does it pull by default?"
    },
    "initFetchOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to pull expressions by default"
    },
    "messages": {
      "$ref": "#/definitions/SchemaMessage"
    },
    "name": {
      "$ref": "#/definitions/SchemaName"
    },
    "toolbar": {
      "$ref": "#/definitions/SchemaCollection",
      "description": "The top area of the page, displayed in the upper right corner when title exists."
    },
    "toolbarClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Configure toolbar container className"
    },
    "definitions": {},
    "interval": {
      "type": "number",
      "description": "Configure the polling interval. After configuration, initApi will poll for loading."
    },
    "silentPolling": {
      "type": "boolean",
      "description": "Whether to load silently, that is to say, do not show the progress"
    },
    "stopAutoRefreshWhen": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Configure conditions to stop polling."
    },
    "showErrorMsg": {
      "type": "boolean",
      "description": "Whether to display error messages. It is displayed by default."
    },
    "cssVars": {
      "description": "css variables"
    },
    "regions": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "aside",
          "body",
          "toolbar",
          "header"
        ]
      },
      "description": "By default, automatic sensing of content is not set to decide whether to display these areas. If configured, the configuration is the main one."
    },
    "pullRefresh": {
      "type": "object",
      "properties": {
        "disabled": {
          "type": "boolean"
        },
        "pullingText": {
          "type": "string"
        },
        "loosingText": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "description": "Pull down to refresh configuration"
    }
  },
  "required": [
    "type"
  ],
  "additionalProperties": false,
  "description": "amis Page renderer. For details, please visit: https://aisuda.bce.baidu.com/amis/zh-CN/components/page",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./PageSchema.json"
}