{
  "type": "object",
  "properties": {
    "$$id": {
      "type": "string",
      "description": "The unique id of the component, mainly used to locate json nodes in the page designer"
    },
    "className": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Container css class name"
    },
    "$ref": {
      "type": "string",
      "description": "Used together with definitions, an infinite loop renderer can be implemented."
    },
    "disabled": {
      "type": "boolean",
      "description": "Whether to disable"
    },
    "disabledOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to disable expressions"
    },
    "hidden": {
      "type": "boolean",
      "description": "Whether to hide"
    },
    "hiddenOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to hide expressions"
    },
    "visible": {
      "type": "boolean",
      "description": "Whether to display"
    },
    "visibleOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions"
    },
    "id": {
      "type": "string",
      "description": "The unique id of the component, mainly used for log collection"
    },
    "onEvent": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "weight": {
            "type": "number"
          },
          "actions": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/ListenerAction"
            }
          },
          "debounce": {
            "$ref": "#/definitions/debounceConfig"
          },
          "track": {
            "$ref": "#/definitions/trackConfig"
          }
        },
        "required": [
          "actions"
        ],
        "additionalProperties": false
      },
      "description": "Event action configuration"
    },
    "static": {
      "type": "boolean",
      "description": "Whether to display statically"
    },
    "staticOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions statically"
    },
    "staticPlaceholder": {
      "type": "string",
      "description": "Static display of empty value placeholder"
    },
    "staticClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display form item class name"
    },
    "staticLabelClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Label class name"
    },
    "staticInputClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Value class name"
    },
    "staticSchema": {},
    "style": {
      "type": "object",
      "description": "Component style"
    },
    "editorSetting": {
      "type": "object",
      "properties": {
        "behavior": {
          "type": "string",
          "description": "Component behavior and usage, such as create, update, remove"
        },
        "displayName": {
          "type": "string",
          "description": "Component name, usually the business name for easy location"
        },
        "mock": {
          "description": "Editor fake data for easy display"
        }
      },
      "description": "Editor configuration, can be ignored at runtime"
    },
    "useMobileUI": {
      "type": "boolean",
      "description": "Can be used to turn off mobile style at the component level"
    },
    "testIdBuilder": {
      "$ref": "#/definitions/TestIdBuilder"
    },
    "type": {
      "type": "string",
      "const": "progress"
    },
    "testid": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "description": "Related field name"
    },
    "value": {
      "type": "number",
      "description": "progress value"
    },
    "mode": {
      "type": "string",
      "enum": [
        "line",
        "circle",
        "dashboard"
      ],
      "description": "Progress bar type"
    },
    "progressClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Progress bar CSS class name"
    },
    "map": {
      "$ref": "#/definitions/ColorMapType",
      "description": "Configure different value segments and prompt users in different styles"
    },
    "showLabel": {
      "type": "boolean",
      "description": "Whether to display value"
    },
    "placeholder": {
      "type": "string",
      "description": "placeholder"
    },
    "stripe": {
      "type": "boolean",
      "description": "Whether to display background interval"
    },
    "animate": {
      "type": "boolean",
      "description": "Whether to display animation (can only be seen when turned on)"
    },
    "strokeWidth": {
      "type": "number",
      "description": "The width of the progress bar line"
    },
    "gapDegree": {
      "type": "number",
      "description": "The notch angle of the dashboard progress bar, which can range from 0 to 295"
    },
    "gapPosition": {
      "type": "string",
      "enum": [
        "top",
        "bottom",
        "left",
        "right"
      ],
      "description": "Dashboard progress bar notch position"
    },
    "valueTpl": {
      "type": "string",
      "description": "Content template functions"
    },
    "threshold": {
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "value": {
              "$ref": "#/definitions/SchemaTpl"
            },
            "color": {
              "type": "string"
            }
          },
          "required": [
            "value"
          ],
          "additionalProperties": false
        },
        {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "value": {
                "$ref": "#/definitions/SchemaTpl"
              },
              "color": {
                "type": "string"
              }
            },
            "required": [
              "value"
            ],
            "additionalProperties": false
          }
        }
      ],
      "description": "threshold"
    },
    "showThresholdText": {
      "type": "boolean",
      "description": "Whether to display the threshold value"
    }
  },
  "required": [
    "mode",
    "type",
    "value"
  ],
  "additionalProperties": false,
  "description": "Progress display control. Documentation: https://aisuda.bce.baidu.com/amis/zh-CN/components/progress",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./ProgressSchema.json"
}