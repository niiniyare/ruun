{
  "type": "object",
  "properties": {
    "$$id": {
      "type": "string",
      "description": "The unique id of the component, mainly used to locate json nodes in the page designer"
    },
    "className": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Container css class name"
    },
    "$ref": {
      "type": "string",
      "description": "Used together with definitions, an infinite loop renderer can be implemented."
    },
    "disabled": {
      "type": "boolean",
      "description": "Whether to disable"
    },
    "disabledOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to disable expressions"
    },
    "hidden": {
      "type": "boolean",
      "description": "Whether to hide"
    },
    "hiddenOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to hide expressions"
    },
    "visible": {
      "type": "boolean",
      "description": "Whether to display"
    },
    "visibleOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions"
    },
    "id": {
      "type": "string",
      "description": "The unique id of the component, mainly used for log collection"
    },
    "onEvent": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "weight": {
            "type": "number"
          },
          "actions": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/ListenerAction"
            }
          },
          "debounce": {
            "$ref": "#/definitions/debounceConfig"
          },
          "track": {
            "$ref": "#/definitions/trackConfig"
          }
        },
        "required": [
          "actions"
        ],
        "additionalProperties": false
      },
      "description": "Event action configuration"
    },
    "static": {
      "type": "boolean",
      "description": "Whether to display statically"
    },
    "staticOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions statically"
    },
    "staticPlaceholder": {
      "type": "string",
      "description": "Static display of empty value placeholder"
    },
    "staticClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display form item class name"
    },
    "staticLabelClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Label class name"
    },
    "staticInputClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Value class name"
    },
    "staticSchema": {},
    "style": {
      "type": "object",
      "description": "Component style"
    },
    "editorSetting": {
      "type": "object",
      "properties": {
        "behavior": {
          "type": "string",
          "description": "Component behavior and usage, such as create, update, remove"
        },
        "displayName": {
          "type": "string",
          "description": "Component name, usually the business name for easy location"
        },
        "mock": {
          "description": "Editor fake data for easy display"
        }
      },
      "description": "Editor configuration, can be ignored at runtime"
    },
    "useMobileUI": {
      "type": "boolean",
      "description": "Can be used to turn off mobile style at the component level"
    },
    "testIdBuilder": {
      "$ref": "#/definitions/TestIdBuilder"
    },
    "type": {
      "type": "string",
      "const": "carousel",
      "description": "Specify as carousel image type"
    },
    "testid": {
      "type": "string"
    },
    "auto": {
      "type": "boolean",
      "description": "Whether to play automatically"
    },
    "interval": {
      "type": [
        "number",
        "string"
      ],
      "description": "Carousel interval"
    },
    "duration": {
      "type": "number",
      "description": "Animation duration"
    },
    "width": {
      "type": "number",
      "description": "set width"
    },
    "height": {
      "type": "number",
      "description": "Set height"
    },
    "controlsTheme": {
      "type": "string",
      "enum": [
        "light",
        "dark"
      ]
    },
    "placeholder": {
      "type": "string",
      "description": "Placeholder"
    },
    "controls": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "dots",
          "arrows"
        ]
      },
      "description": "Configure control content"
    },
    "animation": {
      "type": "string",
      "enum": [
        "fade",
        "slide"
      ],
      "description": "animation type"
    },
    "itemSchema": {
      "$ref": "#/definitions/SchemaCollection",
      "description": "Configure a single rendering template"
    },
    "name": {
      "$ref": "#/definitions/SchemaName"
    },
    "thumbMode": {
      "type": "string",
      "enum": [
        "contain",
        "cover"
      ],
      "description": "Preview mode"
    },
    "options": {
      "type": "array",
      "items": {},
      "description": "Configure fixed values"
    },
    "alwaysShowArrow": {
      "type": "boolean",
      "description": "Whether to always display arrows"
    },
    "multiple": {
      "type": "object",
      "properties": {
        "count": {
          "type": "number"
        }
      },
      "required": [
        "count"
      ],
      "additionalProperties": false,
      "description": "Multi-graph mode configuration items"
    },
    "icons": {
      "type": "object",
      "properties": {
        "prev": {
          "$ref": "#/definitions/SchemaCollection"
        },
        "next": {
          "$ref": "#/definitions/SchemaCollection"
        }
      },
      "additionalProperties": false,
      "description": "Custom arrow icon"
    }
  },
  "required": [
    "type"
  ],
  "additionalProperties": false,
  "description": "Carousel carousel renderer. Documentation: https://aisuda.bce.baidu.com/amis/zh-CN/components/carousel",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./CarouselSchema.json"
}