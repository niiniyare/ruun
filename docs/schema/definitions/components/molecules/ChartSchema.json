{
  "type": "object",
  "properties": {
    "$$id": {
      "type": "string",
      "description": "The unique id of the component, mainly used to locate json nodes in the page designer"
    },
    "className": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Container css class name"
    },
    "$ref": {
      "type": "string",
      "description": "Used together with definitions, an infinite loop renderer can be implemented."
    },
    "disabled": {
      "type": "boolean",
      "description": "Whether to disable"
    },
    "disabledOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to disable expressions"
    },
    "hidden": {
      "type": "boolean",
      "description": "Whether to hide"
    },
    "hiddenOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to hide expressions"
    },
    "visible": {
      "type": "boolean",
      "description": "Whether to display"
    },
    "visibleOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions"
    },
    "id": {
      "type": "string",
      "description": "The unique id of the component, mainly used for log collection"
    },
    "onEvent": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "weight": {
            "type": "number"
          },
          "actions": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/ListenerAction"
            }
          },
          "debounce": {
            "$ref": "#/definitions/debounceConfig"
          },
          "track": {
            "$ref": "#/definitions/trackConfig"
          }
        },
        "required": [
          "actions"
        ],
        "additionalProperties": false
      },
      "description": "Event action configuration"
    },
    "static": {
      "type": "boolean",
      "description": "Whether to display statically"
    },
    "staticOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions statically"
    },
    "staticPlaceholder": {
      "type": "string",
      "description": "Static display of empty value placeholder"
    },
    "staticClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display form item class name"
    },
    "staticLabelClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Label class name"
    },
    "staticInputClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Value class name"
    },
    "staticSchema": {},
    "style": {
      "type": "object",
      "description": "style style"
    },
    "editorSetting": {
      "type": "object",
      "properties": {
        "behavior": {
          "type": "string",
          "description": "Component behavior and usage, such as create, update, remove"
        },
        "displayName": {
          "type": "string",
          "description": "Component name, usually the business name for easy location"
        },
        "mock": {
          "description": "Editor fake data for easy display"
        }
      },
      "description": "Editor configuration, can be ignored at runtime"
    },
    "useMobileUI": {
      "type": "boolean",
      "description": "Can be used to turn off mobile style at the component level"
    },
    "testIdBuilder": {
      "$ref": "#/definitions/TestIdBuilder"
    },
    "type": {
      "type": "string",
      "const": "chart",
      "description": "Specified as chart type"
    },
    "testid": {
      "type": "string"
    },
    "chartTheme": {
      "description": "Chart theme configuration"
    },
    "api": {
      "$ref": "#/definitions/SchemaApi",
      "description": "Chart configuration interface"
    },
    "initFetch": {
      "type": "boolean",
      "description": "Whether to load initially."
    },
    "initFetchOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether initial loading is configured using expressions"
    },
    "config": {
      "description": "Configure echart's config to support data mapping. If data mapping is used, in order to update synchronously, please set trackExpression"
    },
    "trackExpression": {
      "type": "string",
      "description": "Track the expression. If the result of the expression changes, Echart will be updated. This is useful when data mapping is used in config."
    },
    "width": {
      "type": [
        "number",
        "string"
      ],
      "description": "Width setting"
    },
    "height": {
      "type": [
        "number",
        "string"
      ],
      "description": "height setting"
    },
    "interval": {
      "type": "number",
      "description": "Refresh time"
    },
    "name": {
      "$ref": "#/definitions/SchemaName"
    },
    "dataFilter": {
      "$ref": "#/definitions/SchemaFunction"
    },
    "source": {
      "$ref": "#/definitions/SchemaTokenizeableString"
    },
    "disableDataMapping": {
      "type": "boolean",
      "description": "Data mapping in Config is enabled by default. If you want to turn it off, please enable this function."
    },
    "clickAction": {
      "$ref": "#/definitions/ActionSchema",
      "description": "Click behavior configuration can be used to meet drill-down operations, etc."
    },
    "replaceChartOption": {
      "type": "boolean",
      "description": "The default configuration is appended. If you want to completely replace the configuration when updating the configuration, please configure it to true."
    },
    "unMountOnHidden": {
      "type": "boolean",
      "description": "Hide when not visible"
    },
    "mapURL": {
      "$ref": "#/definitions/SchemaApi",
      "description": "Get the address of geo json file"
    },
    "mapName": {
      "type": "string",
      "description": "Map name"
    },
    "loadBaiduMap": {
      "type": "boolean",
      "description": "Load Baidu map"
    }
  },
  "required": [
    "type"
  ],
  "additionalProperties": false,
  "description": "Chart chart renderer. Document: https://aisuda.bce.baidu.com/amis/zh-CN/components/chart",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./ChartSchema.json"
}