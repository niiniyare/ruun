{
  "type": "object",
  "properties": {
    "$$id": {
      "type": "string",
      "description": "The unique id of the component, mainly used to locate json nodes in the page designer"
    },
    "className": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Container css class name"
    },
    "$ref": {
      "type": "string",
      "description": "Used together with definitions, an infinite loop renderer can be implemented."
    },
    "disabled": {
      "type": "boolean",
      "description": "Whether to disable"
    },
    "disabledOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to disable expressions"
    },
    "hidden": {
      "type": "boolean",
      "description": "Whether to hide"
    },
    "hiddenOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to hide expressions"
    },
    "visible": {
      "type": "boolean",
      "description": "Whether to display"
    },
    "visibleOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions"
    },
    "id": {
      "type": "string",
      "description": "The unique id of the component, mainly used for log collection"
    },
    "onEvent": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "weight": {
            "type": "number"
          },
          "actions": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/ListenerAction"
            }
          },
          "debounce": {
            "$ref": "#/definitions/debounceConfig"
          },
          "track": {
            "$ref": "#/definitions/trackConfig"
          }
        },
        "required": [
          "actions"
        ],
        "additionalProperties": false
      },
      "description": "Event action configuration"
    },
    "static": {
      "type": "boolean",
      "description": "Whether to display statically"
    },
    "staticOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Whether to display expressions statically"
    },
    "staticPlaceholder": {
      "type": "string",
      "description": "Static display of empty value placeholder"
    },
    "staticClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display form item class name"
    },
    "staticLabelClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Label class name"
    },
    "staticInputClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Static display of form item Value class name"
    },
    "staticSchema": {},
    "style": {
      "type": "object",
      "description": "Component style"
    },
    "editorSetting": {
      "type": "object",
      "properties": {
        "behavior": {
          "type": "string",
          "description": "Component behavior and usage, such as create, update, remove"
        },
        "displayName": {
          "type": "string",
          "description": "Component name, usually the business name for easy location"
        },
        "mock": {
          "description": "Editor fake data for easy display"
        }
      },
      "description": "Editor configuration, can be ignored at runtime"
    },
    "useMobileUI": {
      "type": "boolean",
      "description": "Can be used to turn off mobile style at the component level"
    },
    "testIdBuilder": {
      "$ref": "#/definitions/TestIdBuilder"
    },
    "type": {
      "type": "string",
      "enum": [
        "list",
        "static-list"
      ],
      "description": "Designated as a List list display control."
    },
    "testid": {
      "type": "string"
    },
    "title": {
      "$ref": "#/definitions/SchemaTpl",
      "description": "title"
    },
    "footer": {
      "$ref": "#/definitions/SchemaCollection",
      "description": "bottom area"
    },
    "footerClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Bottom area class name"
    },
    "header": {
      "$ref": "#/definitions/SchemaCollection",
      "description": "top area"
    },
    "headerClassName": {
      "$ref": "#/definitions/SchemaClassName",
      "description": "Top area class name"
    },
    "listItem": {
      "$ref": "#/definitions/ListItemSchema",
      "description": "Single data display content configuration"
    },
    "source": {
      "$ref": "#/definitions/SchemaTokenizeableString",
      "description": "Data source: bind current environment variables"
    },
    "showFooter": {
      "type": "boolean",
      "description": "Whether to show the bottom"
    },
    "showHeader": {
      "type": "boolean",
      "description": "Whether to display the header"
    },
    "placeholder": {
      "$ref": "#/definitions/SchemaTpl",
      "description": "No data prompt"
    },
    "hideCheckToggler": {
      "type": "boolean",
      "description": "Whether to hide the check box"
    },
    "affixHeader": {
      "type": "boolean",
      "description": "Whether to fix the top"
    },
    "affixFooter": {
      "type": "boolean",
      "description": "Is it solid?"
    },
    "itemCheckableOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Configure whether an item can be clicked"
    },
    "itemDraggableOn": {
      "$ref": "#/definitions/SchemaExpression",
      "description": "Configure whether an item can be sorted by drag-and-drop, provided that the drag-and-drop function is turned on"
    },
    "checkOnItemClick": {
      "type": "boolean",
      "description": "When clicking on a single row in the list, whether to select"
    },
    "valueField": {
      "type": "string",
      "description": "Fields that can be used as values"
    },
    "size": {
      "type": "string",
      "enum": [
        "sm",
        "base"
      ],
      "description": "size"
    },
    "itemAction": {
      "$ref": "#/definitions/ActionSchema",
      "description": "Click behavior on list items"
    }
  },
  "required": [
    "type"
  ],
  "additionalProperties": false,
  "description": "List list display control. Document: https://aisuda.bce.baidu.com/amis/zh-CN/components/card",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./ListSchema.json"
}